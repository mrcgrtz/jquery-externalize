    "window",
    "jquery-plugin",
    "ecosystem:jquery"
  ],
  "version": "4.0.2",
  "author": {
    "name": "Marc Görtz",
    "email": "git@marcgoertz.de",
    "url": "https://marcgoertz.de/"
  },
  "license": "MIT",
  "bugs": "https://github.com/mrcgrtz/jquery-externalize/issues",
  "homepage": "https://github.com/mrcgrtz/jquery-externalize",
  "repository": "https://github.com/mrcgrtz/jquery-externalize",
  "dependencies": {
    "jquery": "^3.6.0"
  },
  "devDependencies": {
    "ava": "^3.15.0",
    "c8": "^7.7.2",
    "eslint": "^8.0.0",
    "eslint-config-jquery": "^3.0.0",
    "jsdom": "^19.0.0"
  },
  "scripts": {
    "test": "eslint *.js && c8 ava",
